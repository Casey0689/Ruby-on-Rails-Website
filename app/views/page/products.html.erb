<h1>Products</h1>

<div class="wrapper">
  <table border="2px solid black">
    <tr>
      <th>Product</th>
      <th>Price</th>
    </tr>
    <% @products.each do |name, price| %>
        <tr>
          <td><%= name %></td>
          <td><%= price %></td>
        </tr>
    <% end %>
  </table>
</div>